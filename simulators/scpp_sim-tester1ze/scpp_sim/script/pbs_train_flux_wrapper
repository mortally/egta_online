# Create a local directory to run and copy your files to local.
# Let PBS handle your output
mkdir /tmp/${PBS_JOBID}
cd /tmp/${PBS_JOBID}
cp -r /home/wellmangroup/many-agent-simulations/scpp_test/* .
time script/batch_train config/ training STRATNAME NUM_AGENT

#Clean up your files
cd
# Retrieve your output
cp -r /tmp/${PBS_JOBID}/pp_data/* /home/wellmangroup/many-agent-simulations/scpp_test/pp_data
/bin/rm -rf /tmp/${PBS_JOBID}	

