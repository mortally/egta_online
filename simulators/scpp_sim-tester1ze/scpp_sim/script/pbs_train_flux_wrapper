#PBS -N scpp-pp-train
#PBS -A wellman_flux
#PBS -l nodes=1:ppn=1,mem=2000mb,walltime=8:00:00
#PBS -q flux
#PBS -M dyoon@umich.edu
#PBS -m abe
#PBS -V
